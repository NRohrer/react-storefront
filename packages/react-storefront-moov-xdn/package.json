{
  "name": "react-storefront-moov-xdn",
  "version": "6.36.4",
  "description": "Extension for running React Storefront apps on Moovweb's XDN platform.",
  "author": "Moovweb",
  "license": "Apache-2.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "mkdir -p dist; yarn build:assets; NODE_ENV=production yarn build:sources",
    "build:sources": "babel src --source-maps --out-dir dist",
    "build:assets": "cp -f .npmrc package.json dist && sync-folders --type copy bin dist",
    "watch": "concurrently 'yarn build:assets --watch' 'yarn build:sources --watch'"
  },
  "bin": {
    "xdn": "bin/index.js"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "moov_builder": "4.0.0-alpha.1",
    "moov_stdlib": "^2.6.33",
    "node-emoji": "^1.10.0",
    "open": "^6.4.0",
    "yargs": "^13.3.0"
  },
  "peerDependencies": {
    "react-storefront": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.1"
  }
}
